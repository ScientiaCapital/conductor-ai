# ===========================================
# Conductor-AI Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values

# Supabase (Required for persistence)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Redis (Required for job state)
REDIS_URL=redis://localhost:6379

# LLM Providers (NO OpenAI - use Gemini or Anthropic)
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_API_KEY=AIza...          # Required for storyboard tools (Gemini)
OPENROUTER_API_KEY=sk-or-...    # Optional for DeepSeek/Qwen

# App Config
APP_ENV=development
API_URL=http://localhost:8000
