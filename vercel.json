{
  "version": 2,
  "builds": [
    {
      "src": "src/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/api.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "REDIS_URL": "@redis_url"
  }
}
